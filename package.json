  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0",
    "npm": "Please use Yarn as your package manager instead."
  },
  "scripts": {
    "lint": "npx eslint --ignore-path .gitignore .",
    "lint:fix": "npx eslint --fix --ignore-path .gitignore .",
    "prettier": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache .",
    "prettier:fix": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache .",
    "prepare": "husky install",
    "commitlint": "commitlint --from=HEAD"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.0.1",
